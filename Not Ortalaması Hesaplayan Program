import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    //not ortalaması hesaplayan program
        int mat,turkce,tarih,biyoloji,fizik,muzik;
        Scanner okur=new Scanner(System.in);
        System.out.print("Matematik notunuzu girin");
        mat=okur.nextInt();

        System.out.print("Türkçe notunuzu girin");
        turkce=okur.nextInt();

        System.out.print("Tarih notunuzu girin");
        tarih=okur.nextInt();

        System.out.print("Biyoloji notunuzu girin");
        biyoloji=okur.nextInt();

        System.out.print("Fizik notunuzu girin");
        fizik=okur.nextInt();

        System.out.print("muzik notunuzu girin");
        muzik=okur.nextInt();

        double ortalama = (mat+turkce+tarih+biyoloji+fizik+muzik)/6;
        System.out.println(ortalama);
        System.out.println(ortalama>=60? "gectiniz":"kaldınız");

    }
}
